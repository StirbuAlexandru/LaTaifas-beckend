# ===========================================
# ING WEBPAY CONFIGURATION
# ===========================================
# Credențiale primite de la ING după semnarea contractului

# User API ING WebPay (Ex: webpay_user_ron)
ING_WEBPAY_USER=your_ing_user_here

# Parola API ING WebPay
ING_WEBPAY_PASSWORD=your_ing_password_here

# URL de bază al site-ului (folosit pentru returnUrl și failUrl)
NEXT_PUBLIC_BASE_URL=https://yourdomain.com

# ===========================================
# STRIPE CONFIGURATION (OPTIONAL - BACKUP)
# ===========================================
# Dacă dorești să păstrezi Stripe ca metodă alternativă

NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...

# ===========================================
# SUPABASE CONFIGURATION
# ===========================================
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# ===========================================
# OTHER CONFIGURATION
# ===========================================
# Add other environment variables here
